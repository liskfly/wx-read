.light {
  width: 100vw;
  height: 100vh;
  overflow: auto;
  background-color: #f7f7f7;
}
.light .place-bottom {
  width: 100vw;
  height: 250rpx;
  position: fixed;
  bottom: 0;
}
.light .book-bottom {
  width: 100vw;
  background-color: #f6f4ec;
  display: flex;
  position: fixed;
  bottom: 0;
}
.light .book-bottom .previous {
  flex: 1;
  font-size: 40rpx;
  margin: 40rpx 0;
  text-align: center;
}
.light .book-bottom .book-catalogue {
  flex: 1;
  font-size: 40rpx;
  margin: 40rpx 0;
  text-align: center;
}
.light .book-bottom .next {
  flex: 1;
  font-size: 40rpx;
  margin: 40rpx 0;
  text-align: center;
}
.light .catalogue-title {
  width: 50vw;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #f7f7f7;
  overflow: auto;
}
.light .catalogue-title .title {
  font-size: 60rpx;
}
.light .catalogue-title .list {
  width: 100%;
  margin-top: 10rpx;
  padding-bottom: 10rpx;
  border-bottom: 0.1rpx solid #cfcfcf;
}
.light .catalogue-title .list .item-title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.light .catalogue-title .list .light {
  color: red;
}
.light .top {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  background-color: #f7f7f7;
}
.light .head {
  width: 100vw;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #aaaaaa;
  position: -webkit-sticky;
  position: sticky;
  background-color: #f7f7f7;
}
.light .head image {
  width: 30px;
  height: 30px;
  margin-left: 20rpx;
}
.light .head {
  width: 100vw;
}
.light .m-mb {
  height: calc(100vh - 60px);
  margin: 0 16px;
}
.light .m-mb .m-content {
  height: 100%;
}
.light .m-mb .m-content .p {
  margin-bottom: 20rpx;
